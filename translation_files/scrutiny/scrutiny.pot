msgstr ""

#. type: Plain text
#: src/scrutiny/DESCRIPTION.md:8
msgid ""
"Scrutiny is a simple but focused application, with a couple of core features:"
msgstr ""

#. type: Bullet: '- '
#: src/scrutiny/DESCRIPTION.md:17
msgid "Web UI Dashboard - focused on Critical metrics"
msgstr ""

#. type: Bullet: '- '
#: src/scrutiny/DESCRIPTION.md:17
msgid "`smartd` integration (no re-inventing the wheel)"
msgstr ""

#. type: Bullet: '- '
#: src/scrutiny/DESCRIPTION.md:17
msgid "Auto-detection of all connected hard-drives"
msgstr ""

#. type: Bullet: '- '
#: src/scrutiny/DESCRIPTION.md:17
msgid "S.M.A.R.T metric tracking for historical trends"
msgstr ""

#. type: Bullet: '- '
#: src/scrutiny/DESCRIPTION.md:17
msgid "Customized thresholds using real world failure rates"
msgstr ""

#. type: Bullet: '- '
#: src/scrutiny/DESCRIPTION.md:17
msgid "Temperature tracking"
msgstr ""

#. type: Bullet: '- '
#: src/scrutiny/DESCRIPTION.md:17
msgid "Provided as an all-in-one Docker image (but can be installed manually)"
msgstr ""

#. type: Bullet: '- '
#: src/scrutiny/DESCRIPTION.md:17
msgid "Configurable Alerting/Notifications via Webhooks"
msgstr ""

