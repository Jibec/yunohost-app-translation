#. type: Plain text
#: src/gemserv/ADMIN.md:4
msgid ""
"To add a gemini capsule, create a `/etc/gemserv/config.d/example.toml` file "
"as following:"
msgstr ""

#. type: Plain text
#: src/gemserv/ADMIN.md:6
msgid "Minimal version :"
msgstr ""

#. type: Fenced code block (toml)
#: src/gemserv/ADMIN.md:7
#, no-wrap
msgid ""
"[[server]]\n"
"hostname = \"yourdomain.org\"\n"
"dir = \"/opt/yunohost/gemserv\"\n"
"key = \"/etc/yunohost/certs/yourdomain.org/key.pem\"\n"
"cert = \"/etc/yunohost/certs/yourdomain.org/crt.pem\"\n"
msgstr ""

#. type: Plain text
#: src/gemserv/ADMIN.md:15
msgid "Extended version :"
msgstr ""

#. type: Fenced code block (toml)
#: src/gemserv/ADMIN.md:15
#, no-wrap
msgid ""
"[[server]]\n"
"hostname = \"yourdomain.org\"\n"
"dir = \"/opt/yunohost/gemserv\"\n"
"key = \"/etc/yunohost/certs/yourdomain.org/key.pem\"\n"
"cert = \"/etc/yunohost/certs/yourdomain.org/crt.pem\"\n"
"# index is optional but defaults to index.gemini. The server will serve files\n"
"# ending in gemini or gmi.\n"
"index = \"index.gmi\"\n"
"# lang is optional\n"
"lang = \"en\"\n"
"# cgi is optional bool\n"
"cgi = true\n"
"# cgipath is optional and only checked if cgi is true. It restricts cgi to only\n"
"# this directory.\n"
"cgipath = \"/path/to/cgi-bin/\"\n"
"# scgi is optional\n"
"scgi = { \"/scgi\" = \"localhost:4000\" }\n"
"# cgienv is optional\n"
"cgienv = { \"GIT_PROJECT_ROOT\" = \"/srv/git\" }\n"
"# usrdir is optional. it'll look in each user's ~/public_gemini\n"
"usrdir = true\n"
"# proxy is optional\n"
"# path is what comes after the hostname e.g. example.com/path\n"
"proxy = { path = \"localhost:1966\" }\n"
"# proxy_all is optional\n"
"# It will send all requests to the specified server. It also supports streamming.\n"
"proxy_all = \"localhost:1967\"\n"
"# redirect is optional\n"
"redirect = { \"/redirect\" = \"/\", \"/newdomain\" = \"gemini://example.net\" }\n"
msgstr ""

#. type: Plain text
#: src/gemserv/ADMIN.md:47
msgid ""
"And then create the folder `/opt/yunohost/gemserv` and put your files in "
"that folder."
msgstr ""

